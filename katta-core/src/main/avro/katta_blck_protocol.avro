{
    "namespace":"com.ivyft.katta.protocol",
    "doc":"Katta Process Interface",
    "protocol":"KattaClientProtocol",
    "name":"KattaClient",

    "types":[
            {
                 "name":"Message",
                 "type":"record",
                 "fields":[
                     {
                         "name":"indexId",
                         "type":"string"
                     },
                     {
                         "name":"rowId",
                         "type":"string"
                     },
                     {
                         "name":"payload",
                         "type": "bytes"
                     }
                 ]
            }],

    "messages":{
        "add" :{
            "doc":"插入单条数据",
            "request": [{"name" : "message", "type" : "Message"}],
            "response": "int"
        },
        "addList" :{
             "doc":"批次插入(List)",
             "request": [{"name" : "messages", "type":{"type": "array", "items": "Message"}}],
             "response": "int"
        },
        "comm" :{
             "doc":"提交",
             "request": [{
                 "name":"indexId",
                 "type":"string"
             }],
             "response": "null"
        },
        "roll" :{
             "doc":"提交失败回滚",
             "request": [{
                 "name":"indexId",
                 "type":"string"
             }],
             "response": "null"
        },
        "cls" :{
             "doc":"关闭接口",
             "request": [],
             "response": "null"
        }
    }
}